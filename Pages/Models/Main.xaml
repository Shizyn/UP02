<Page x:Class="UP02.Pages.Models.Main"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:UP02.Pages.Models"
      mc:Ignorable="d" 
        d:DesignHeight="450" d:DesignWidth="800"
  Title="Main">

    <Grid>
        <Grid Background="#9EA1A2">
            <ScrollViewer Margin="10,10,10,50">
                <StackPanel x:Name="modelsParent" Margin="5" Background="Gray">

                </StackPanel>
            </ScrollViewer>
            <Button Content="Назад" Width="100" Height="30" FontSize="14" Background="#0060AC" Foreground="White" Click="GetBackButton_Click" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" />
            <Frame x:Name="frame" NavigationUIVisibility="Hidden"/>
        </Grid>
    </Grid>
</Page>
